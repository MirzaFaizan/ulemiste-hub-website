{
    "name": "vntrs-hub-website",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "NODE_OPTIONS='--inspect' next dev -p 3000",
        "build": "next build",
        "start": "next start",
        "prisma-generate": "npx prisma generate --schema ./src/database/schema.prisma",
        "prisma-introspect": "npx prisma introspect --schema ./src/database/schema.prisma",
        "prisma-push": "npx prisma db push --schema ./src/database/schema.prisma",
        "prisma-migrate": "npx prisma migrate dev --schema ./src/database/schema.prisma",
        "prisma-studio": "npx prisma studio --schema ./src/database/schema.prisma",
        "prisma-seed": "npx prisma db seed --schema ./src/database/schema.prisma --preview-feature",
        "prisma-sync": "yarn prisma-push & yarn prisma-generate",
        "test": "./node_modules/.bin/cypress open",
        "lint": "eslint --ext .js,.ts,.tsx src",
        "format": "prettier --write --check src",
        "prepare": "husky install",
        "cypress-install": "npx cypress install",
        "cypress-run": "npx cypress run --headless"
    },
    "dependencies": {
        "@apollo/client": "^3.3.20",
        "@headlessui/react": "^1.2.0",
        "@prisma/client": "^2.22.1",
        "axios": "^0.21.1",
        "dayjs": "^1.10.4",
        "graphql": "^15.5.1",
        "next": "^11.0.0",
        "next-translate": "^1.0.6",
        "ramda": "^0.27.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-hook-form": "^7.4.0",
        "react-icons": "^4.2.0",
        "react-query": "^3.15.2",
        "react-scroll": "^1.8.3",
        "superjson": "^1.7.4",
        "zustand": "^3.5.0"
    },
    "devDependencies": {
        "@types/react": "^17.0.5",
        "@typescript-eslint/eslint-plugin": "^4.23.0",
        "@typescript-eslint/parser": "^4.23.0",
        "autoprefixer": "^10.2.5",
        "babel-plugin-superjson-next": "^0.3.0",
        "cypress": "^7.2.0",
        "eslint": "^7.26.0",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "husky": "^6.0.0",
        "lint-staged": "^11.0.0",
        "postcss": "^8.2.15",
        "prettier": "^2.3.0",
        "prisma": "^2.22.1",
        "tailwindcss": "^2.1.2",
        "typescript": "^4.3.4"
    }
}
